{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"routes": [
		{
			"source":"(.*)(xsjs)",
			"destination":"datajs_api",
			"csrfProtection":false,
			"authenticationType":"xsuaa"
			
		},{
			"source":"(.*)(.xsodata)",
			"destination":"datajs_api",
			"authenticationType":"xsuaa"
		},{ 
			"source": "^/user.xsodata/.*$", 
			"destination": "datajs_api" 
			
		}
		]
}