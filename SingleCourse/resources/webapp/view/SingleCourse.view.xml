<mvc:View controllerName="SC.SingleCourse.controller.SingleCourse" 
	xmlns:mvc="sap.ui.core.mvc" 
displayBlock="true" 
xmlns:core="sap.ui.core"
xmlns="sap.m"
xmlns:tnt="sap.tnt"
xmlns:l="sap.ui.layout"
height="100%">
	<Shell id="shell">
		<App id="app">
			<pages>
			<Page title="Title">		
		<customHeader>
		<Bar>
		<contentLeft>
		<Title level="H1" text = "Course Title:" />
		</contentLeft>
		<contentRight>
		<Button text="Menu" type="Accept"></Button>
		</contentRight>
		</Bar>
		</customHeader>
					<content>
	
					<Toolbar>
					<Title level="H2" text="Time Filter:" width="100%" />
					<ToolbarSpacer />
				    </Toolbar>
				     <Title level="H3" text="Time Range:"  width="100%" />
				    <DateRangeSelection
				    	width="20%"
		         	id="DRS2"
			        change="handleChange"/>
			         <Toolbar>
					<Title level="H3" text="Table of Participants:" />
					<ToolbarSpacer />
				    </Toolbar>
				    <SearchField width="100%" class="sapUiSmallMargin" />
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>